{
  "ftp": {
    "host": "",
    "username": "",
    "password": "",
    "path": ""
  },
  "ftpFolder": "dist",
  "alias": {
    "@": "src",
    "calf-ui": "~@cardniu/calf-ui/lib"
  },
  "proxy": {
    "open": false,
    "root": "http://www.baidu.com",
    "apis": ["/login", "/login-out"]
  },
  "devServer": {
    "host": "localhost",
    "port": 8080,
    "autoOpenBrowser": true,
    "errorOverlay": true,
    "poll": false
  },
  "dev": {
    "assetsSubDirectory": "static",
    "assetsPublicPath": "/",
    "cssSourceMap": true,
    "devtool": "#source-map",
    "useEslint": true,
    "showEslintErrorsInOverlay": false
  },
  "build": {
    "assetsSubDirectory": "static",
    "assetsPublicPath": "http://www.baidu.com/",
    "productionSourceMap": false,
    "devtool": "#source-map",
    "bundleAnalyzerReport": true,
    "bundleAnalyzerPort": 8090
  }
}
